<h1>Listing product_cates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Product cate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_cates.each do |product_cate| %>
      <tr>
        <td><%= product_cate.name %></td>
        <td><%= product_cate.product_cate_id %></td>
        <td><%= link_to 'Show', product_cate %></td>
        <td><%= link_to 'Edit', edit_product_cate_path(product_cate) %></td>
        <td><%= link_to 'Destroy', product_cate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product cate', new_product_cate_path %>
